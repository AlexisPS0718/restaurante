generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  userId Int @id @default(autoincrement())
  username String
  password String
  roleId Int
  role Roles @relation(fields: [roleId], references: [roleId])

}

model Roles {
  roleId Int @id @default(autoincrement())
  roleName String
  users Users[]
}
